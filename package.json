{
  "name": "repo",
  "private": true,
  "engines": {
		"node": ">=18",
		"pnpm": ">=8"
	},
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "website": "pnpm dev --filter website",
    "directory": "pnpm dev --filter devnet-directory",
    "run:directory": "node ./apps/devnet-directory/build/index.js",
    "docker:directory": "docker build -t devnet-directory ./ -f ./dockerfile.directory",
    "docker:build:directory": "docker build -t devnet-directory ./ -f ./apps/devnet-directory/Dockerfile",
    "docker:up:directory": "docker-compose -f ./apps/devnet-directory/docker-compose.yml -p devnet-directory up -d",
    "docker:down:directory": "docker-compose -p devnet-directory down --remove-orphans --volumes",
    "db:push": "cd packages/prisma-shared && npx prisma db push",
    "db:browse": "cd packages/prisma-shared && npx prisma studio",
    "docker:up": "docker-compose -p 76devs up -d",
		"docker:down": "docker-compose -p 76devs down --remove-orphans --volumes"
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "turbo": "^2.0.4",
    "typescript": "^5.4.5"
  },
  "packageManager": "pnpm@8.15.6"
}
